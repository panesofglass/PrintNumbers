<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>PrintNumbersLib</PackageId>
    <Version>1.0.0</Version>
    <Authors>Ryan Riley</Authors>
    <Company>Panes of Glass</Company>
    <PackageDescription>Generate a sequence of formatted numbers from 1 to a specified upper bound provided a set of formatting rules.</PackageDescription>
    <RepositoryUrl>https://github.com/panesofglass/PrintNumbers</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="PrintNumbersLib.Tests" />
  </ItemGroup>

</Project>
